{
  "name": "krownify",
  "version": "0.0.1",
  "description": "Krownify web client for classifying Danish coins using TensorFlow.js",
  "keywords": [
    "react",
    "tensorflow",
    "typescript",
    "machine learning",
    "firebase"
  ],
  "author": "Christoffer Kjeldgaard Petersen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ckjeldgaard/krownify.git"
  },
  "homepage": "https://github.com/ckjeldgaard/krownify",
  "scripts": {
    "build": "npm run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rm -f -r -d dist",
    "lint": "tslint './src/**/*.ts*' './tests/**/*.tsx' --format stylish --force",
    "start": "npm run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev.js",
    "start-prod": "npm run build && node express.js",
    "test": "jest --watch --coverage --config=configs/jest.json"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@types/croppie": "^2.5.3",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.15",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/react-global-configuration": "^1.3.1",
    "@types/react-modal": "^3.6.0",
    "@types/react-router-dom": "^4.3.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.4",
    "croppie": "^2.6.2",
    "css-loader": "^2.0.1",
    "express": "^4.16.4",
    "file-loader": "^2.0.0",
    "firebase": "^5.7.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^23.6.0",
    "node-sass": "^4.11.0",
    "react": "^16.6.3",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.6.3",
    "react-global-configuration": "^1.3.0",
    "react-hot-loader": "^4.6.1",
    "react-modal": "^3.8.1",
    "react-router-dom": "^4.3.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "uuid": "^3.3.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.5"
  },
  "dependencies": {}
}
